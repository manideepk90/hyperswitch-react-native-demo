{
  "name": "hyperswitchReactNativeDemo",
  "jsx": {
    "version": 4
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@rescript/core",
    "rescript-react-native",
    "@rescript/react",
    "hyperswitch-sdk-react-native/src"
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx"
  },
  "bsc-flags": ["-open RescriptCore"]
}
